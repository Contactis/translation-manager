  },
  "properties": {
    "translatedPhrase": {
      "type": "string",
      "required": true
    },
    "pluralForm": {
      "type": "number"
    },
    "description": {
      "type": "string"
    },
    "translationsKeyId": {
      "type": "number",
      "required": true
    },
    "languageId": {
      "type": "number",
      "required": true
    },
    "statusId": {
      "type": "number",
      "required": true
    },
    "lastModifiedBy": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
