{
  "name": "Project",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "defaultLanguageId": {
      "type": "number",
      "required": true
    },
    "description": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "defaultLanguage": {
      "type": "belongsTo",
      "model": "Language",
      "foreignKey": "defaultLanguageId"
    },
    "languages": {
      "type": "hasMany",
      "model": "Language",
      "foreignKey": "",
      "through": "ProjectLanguage"
    },
    "accounts": {
      "type": "hasMany",
      "model": "Account",
      "foreignKey": "",
      "through": "ProjectAccount"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
